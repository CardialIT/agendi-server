// Tabela = Model

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Professional {
  id               Int    @id @default(autoincrement())
  nameProfessional String
  profession       String

  @@map("professionals")
}

model Type {
  id   Int    @id @default(autoincrement())
  name String
}

model Client {
  id    Int    @id @default(autoincrement())
  name  String @unique
  email String
  phone String
}

model Scheduling {
  id           Int    @id @default(autoincrement())
  profissional String
  type         String
  client       String
  date         String
  hour         String
}
